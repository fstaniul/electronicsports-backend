{
  "restApiRoot": "/api",
  "host": "electronicsports.pl",
  "port": 3000,
  "httpPort": 80,
  "https": false,
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": true,
      "normalizeHttpPath": true,
      "xml": false
    },
    "json": {
      "strict": true,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  }
}
